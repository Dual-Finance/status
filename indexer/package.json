{
  "name": "ledger",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "index": "ts-node src/bin/index/main.ts",
    "export": "ts-node src/bin/export/main.ts",
    "ledgerd": "ts-node src/bin/ledgerd/main.ts",
    "deploy": "fly deploy && sleep 30 && fly app restart",
    "sql": "fly pg connect -a cks-postgres -d cks_ledger",
    "proxy": "fly proxy -a cks-postgres 5432",
    "dump": "pg_dump -x -c --if-exists -f prod.dump $DATABASE_URL",
    "replay": "psql -d postgres -f prod.dump"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.3.3",
    "@types/node": "^20.3.1",
    "@types/qs": "^6.9.7",
    "ley": "^0.8.1",
    "mocha": "^10.2.0",
    "pg-connection-string": "^2.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@blockworks-foundation/mango-v4": "^0.19.33",
    "@coral-xyz/anchor": "0.27.0",
    "@openbook-dex/openbook": "^0.0.8",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.77.3",
    "alchemy-sdk": "^2.9.1",
    "csv-parse": "^5.4.0",
    "dayjs": "^1.11.8",
    "ethers": "^6.6.7",
    "got": "11.8.2",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "postgres": "^3.3.5",
    "qs": "^6.11.2"
  },
  "resolutions": {
    "**/@solana/web3.js/node-fetch": "npm:@blockworks-foundation/node-fetch@2.6.11",
    "**/cross-fetch/node-fetch": "npm:@blockworks-foundation/node-fetch@2.6.11"
  }
}
